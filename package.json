{
  "name": "mongodb-version-manager",
  "version": "0.7.2",
  "description": "Cross-platform helper for managing and installing MongoDB",
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/version-manager.git"
  },
  "homepage": "https://github.com/mongodb-js/version-manager",
  "bin": {
    "mongodb-version-manager": "bin/mongodb-version-manager.js",
    "m": "bin/m.js"
  },
  "scripts": {
    "fmt": "mongodb-js-fmt lib/*.js",
    "check": "mongodb-js-precommit",
    "test": "mocha",
    "ci": "npm run check && npm run coverage",
    "coverage": "./node_modules/istanbul/lib/cli.js cover _mocha -- -R spec ./test/*.test.js"
  },
  "check": {
    "ignore": [
      "coverage/{*,**/*}"
    ]
  },
  "precommit": [
    "check"
  ],
  "devDependencies": {
    "coveralls": "^2.11.4",
    "eslint-config-mongodb-js": "^0.1.6",
    "istanbul": "^0.3.22",
    "mocha": "~2.3.3",
    "mongodb-js-fmt": "0.0.3",
    "mongodb-js-precommit": "^0.2.2",
    "pre-commit": "^1.1.1"
  },
  "dependencies": {
    "async": "~1.4.2",
    "debug": "~2.2.0",
    "docopt": "~0.6.2",
    "fs-extra": "~0.24.0",
    "lodash.defaults": "^3.1.2",
    "lodash.difference": "^3.2.2",
    "mongodb-download-url": "0.1.1",
    "mongodb-version-list": "0.0.3",
    "progress": "~1.1.8",
    "request": "~2.64.0",
    "semver": "~5.0.3",
    "tar": "~2.2.1",
    "tildify": "~1.1.2",
    "untildify": "~2.1.0",
    "unzip": "~0.1.11",
    "which": "~1.2.0"
  },
  "license": "Apache-2.0"
}
